{
  "name": "sushi-sh",
  "description": "CLI for Sushiswap protocol",
  "version": "1.0.0",
  "author": "SEE  CONTRIBUTORS",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "sushi": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/manifoldfinance/sushi-sh/issues"
  },
  "dependencies": {
    "@graphql-codegen/typescript-oclif": "^2.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "eslint-plugin-vue": "^7.9.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.4"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.21.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/near-operation-file-preset": "^2.1.2",
    "@graphql-codegen/typescript": "^2.1.2",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^11.0.3",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/awilmoth/sushi",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sushi",
    "macos": {
      "identifier": "com.sushi.cli"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awilmoth/sushi.git"
  },
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "exit 0",
    "version": "oclif-dev readme && git add README.md",
    "codegen": "graphql-codegen --config codegen.yml",
    "release": "oclif-dev pack:macos && oclif-dev pack:win && npm publish --access public",
    "lint": "eslint src --ext .ts"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  }
}
